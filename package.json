{
    "name": "orangehrm-qa-automation",
    "version": "1.0.0",
    "description": "Playwright-based QA tests for the OrangeHRM demo app",
    "scripts": {
        "test": "playwright test",
        "test:headed": "playwright test --headed",
        "test:debug": "playwright test --debug",
        "test:ui": "playwright test --ui",
        "test:report": "playwright show-report",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "playwright",
        "typescript",
        "qa",
        "automation"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@playwright/test": "^1.40.0",
        "@types/node": "^20.10.0",
        "typescript": "^5.3.0"
    }
}